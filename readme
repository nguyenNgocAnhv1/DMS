dotnet add package Microsoft.Data.SqlClient                             
dotnet add package  Microsoft.EntityFrameworkCore                       
dotnet add package  Microsoft.EntityFrameworkCore.SqlServer             
dotnet add package  Microsoft.EntityFrameworkCore.Tools                 
dotnet add package  Microsoft.VisualStudio.Web.CodeGeneration.Design    
dotnet aspnet-codegenerator area Database
 dotnet aspnet-codegenerator controller -name BoxController -namespace App.Controllers.Boxs -m App.Models.Box -udl -dc App.Models.AppDbContext -outDir Areas/Box/Controllers                                       